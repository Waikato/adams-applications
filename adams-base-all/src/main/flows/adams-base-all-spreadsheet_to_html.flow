adams.flow.control.Flow
	-annotation
	Turns a spreadsheet into an HTML page.
	-actor
		adams.flow.source.FileSupplier
			-file
			${FLOWS}/data/bolts.csv
	-actor
		adams.flow.transformer.SpreadSheetFileReader
			-reader
				adams.data.io.input.CsvSpreadSheetReader
	-actor
		adams.flow.transformer.Convert
			-conversion
				adams.data.conversion.SpreadSheetToHTML
					-title
					UCI dataset "bolts"
					-html-before-table
					<h2>UCI dataset "bolts"</h2>
					-html-after-table
					<p>Generated by ADAMS</p>
	-actor
		adams.flow.sink.Display
	-flow-execution-listener
		adams.flow.execution.NullListener

